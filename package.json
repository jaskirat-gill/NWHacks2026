{
  "name": "realitycheck",
  "version": "1.0.0",
  "description": "Security and education-focused social media threat detector",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc -p tsconfig.electron.json",
    "build:electron": "tsc -p tsconfig.electron.json",
    "preview": "vite preview",
    "electron": "npm run build:electron && electron .",
    "electron:dev": "concurrently \"vite\" \"npm run build:electron && wait-on http://localhost:5173 && electron .\"",
    "start": "npm run build && electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaskirat-gill/NWHacks2026.git"
  },
  "keywords": [
    "electron",
    "security",
    "threat-detection",
    "ai-detection"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jaskirat-gill/NWHacks2026/issues"
  },
  "homepage": "https://github.com/jaskirat-gill/NWHacks2026#readme",
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "wait-on": "^9.0.3"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  }
}
